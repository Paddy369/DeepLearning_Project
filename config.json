{
    "batch_size_train": 64,
    "batch_size_val": 8,
    "batch_size_test": 4,
    "epochs": 10,
    "image_size": 128,
    "augmentation": false,
    "weight_decay": false,
    "learning_rate_decay": false,
    "layers": [
        {
            "type": "flatten"
        },
        {
            "type": "batch_normalization"
        },
        {
            "type": "dense",
            "size": 256,
            "activation": "relu"
        },
        {
            "type": "dropout",
            "value": 0.4 
        },
        {
            "type": "dense",
            "size": 128,
            "activation": "relu"
        },
        {
            "type": "dropout",
            "value": 0.1 
        },
        {
            "type": "dense",
            "size": 15,
            "activation": "softmax"
        }
    ]
}