{
    "batch_size_train": 64,
    "batch_size_val": 8,
    "batch_size_test": 1,
    "batch_size_predict": 8,
    "epochs": 10,
    "image_size": 256,
    "augmentation": true,
    "weight_decay": false,
    "learning_rate_decay": false,
    "layers": [
        {
            "type": "flatten"
        },
        {
            "type": "batch_normalization"
        },
        {
            "type": "dense",
            "size": 256,
            "activation": "relu"
        },
        {
            "type": "dropout",
            "value": 0.4, 
            "seed": 1
        },
        {
            "type": "dense",
            "size": 128,
            "activation": "relu"
        },
        {
            "type": "dropout",
            "value": 0.1, 
            "seed": 1
        },
        {
            "type": "dense",
            "size": 15,
            "activation": "softmax"
        }
    ]
}